<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  app:key="a0"
    xmlns:app="http://schemas.android.com/apk/res-auto">
  <PreferenceScreen
        app:title="@string/pref_category_schedule"
        app:key="q0"
        app:persistent="false">
    <SwitchPreferenceCompat
        app:key="ScheduleShowEmptyLessons"
        app:title="@string/pref_title_schedule_empty_lessons"
        app:summary="@string/pref_summary_schedule_empty_lessons"
        app:defaultValue="false"/>
    <SwitchPreferenceCompat
        app:key="ScheduleShowColoredLessons"
        app:title="@string/pref_title_schedule_colored_lessons"
        app:summary="@string/pref_summary_schedule_colored_lessons"
        app:defaultValue="true"/>
</PreferenceScreen>
</PreferenceScreen>
